		"babel-jest": "^25.1.0",
		"core-js": "^3.6.4",
		"css-loader": "^3.4.2",
		"css-mqpacker": "^7.0.0",
		"cssnano": "^4.1.10",
		"jest": "^25.1.0",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-test-renderer": "^16.12.0",
