{
	"title": "Semantic new tab for Firefox",
	"name": "custom-newtab-firefox",
	"version": "0.1.0",
	"description": "Customizable Semantic new tab extension",
	"author": "Daniel Egortsev",
	"scripts": {
		"dev": "webpack-dev-server --open --mode development",
		"clean": "rm -rf ./build",
		"watch": "webpack --watch",
		"try:pre": "web-ext run -s ./build",
		"try": "npm run build && npm run try:pre",
		"build": "npm run clean && webpack -p",
		"lint": "npm run build && web-ext lint -s ./build",
		"xpi": "npm run build && web-ext build -s ./build",
		"test": "jest"
	},
	"dependencies": {
